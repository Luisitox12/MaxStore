  <section id="home" class="sec-main">
    <h1 class="main-heading">Todos nuestros Productos</h1>
  </section>

 <section id="about" class="sec-about">
  <div class="container">

    <% Object.keys(catalogo).forEach(categoria => { %>
        <h2><%= categoria %></h2>
        <div class="row">
            <% catalogo[categoria].forEach(producto => { %>
                <div class="col-md-4">
                    <div class="thumbnail">
                        <img src="<%= producto.imagen %>" alt="" class="img-responsive">
                        <div class="caption">
                            <h3><%= producto.nombre %></h3>
                            <p>Precio: $<%= producto.precio %></p>
                            <a href="/add-to-cart/<%= producto.id %>" class="btn btn-success">AÃ±adir al carrito</a>
                        </div>
                    </div>
                </div>
            <% }); %>
        </div>
    <% }); %>
</div>
